eta = vector(length=n)
y[1] = eta[1]
eta = calc_eta(Qobs,Qh,param,heteroModel)
mu0 = param$mean_eta_0
mu1 = param$mean_eta_1
mu = mu0+Qh*mu1
phi = param$rho
for (i in 2:n){
y[i] = (eta[i]-mu[i]) - (phi*(eta[i-1]-mu[i]))
}
return(y)
}
